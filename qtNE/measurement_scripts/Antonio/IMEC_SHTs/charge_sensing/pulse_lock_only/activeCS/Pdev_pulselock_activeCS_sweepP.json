{
  "Sample name": "imec QBC3 - chip3",
  "Comments": "VSD=10 mv", 
  
  "Instruments": { 
  
	  "Classes":{
				"NEtransport":{
								  "location": "custom_loops.NEInstruments"
								},
				"CShires":{
								  "location": "custom_loops.NEInstruments",
								  "parameters needed":["inputs", "outputs"],
								  "init": {
											"ID_Is": "Idc1",
											"ID_coarse": "Pcoarse",
											"ID_fine": "Pfine"
											}
							}
				},
  
	  "Inputs": {
		  "S3b":{	  
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["VSDe", "Pcoarse"],
			  "DAC number": [2, 1],
			  "gains": [0.1, 5.0],
			  "unit": "mV",
			  "metaparameters":{
								"rate delay": [0.5] } 
			},

		  "S2d":{
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["LL", "RL", "Pfine", "LB", "RB", "C", "G1", "G2", "G3", "G4", "R"],
			  "DAC number": [12, 13, 14, 15, 16, 4, 5, 6, 7, 8, 9],
			  "gains": [5.0, 5.0, 0.05, 1.0, 1.0, 1.0, 5.0, 5.0, 5.0, 5.0, 5.0],
			  "unit": "mV",
			  "metaparameters":{
								"rate delay": [0.5, 0.5] } 
			} 
				},
					
	  "Outputs":{
		  "Keithley4200":{
					  "class": "NEtransport",
					  "component": "keith1",
					  "name": "Idc1",
					  "gain": 1e-1,
					  "label": "SD current", 
					  "unit": "nA",
					  "metaparameters":{} }, 
		  "SR830":{
					"class": "NEtransport",
					"component": "lock_pulse",
					"parameters": ["R", "P"],
					"labels": ["R_pulse", "Ph_pulse"],
					"units": ["V", "deg"],
					"init":{
							"reference_source": "external",
							"time_constant": 30e-3,
							"reserve": "normal",
							"phase": "auto"
							}	}
				  }},

  "Experiment":{
											
      "Sweep loop":{
					"ID": ["Pcoarse"],
                    "sweepback": false
                    },
      "Step loop":{
                    "to use": false,	  	
                    "ID": ["G3"]
                  },
				  
	  
	  "Class settings":{
				  "NEtransport":{
								  "Voltage ranges": {
													 "VSDe": {
															"begin": 0.5,
															"end": 0.5,
															"stepsize": 10},
			  										 "Pcoarse": {
															"begin": -4000,
															"end": -4600,
															"stepsize": 3},
													 "Pfine": {
															"begin": 0,
															"end": 0,
															"stepsize": 3},
													 "LL": {
															"begin": -4500,
															"end": -4500,
															"stepsize": 10},
													"RL": {
															"begin": -4500,
															"end": -4500,
															"stepsize": 10},
													 "LB": {
															"begin": -576,
															"end": -576,
															"stepsize": 3},
													 "RB": {
															"begin": -670,
															"end": -670,
															"stepsize": 3},
															 
													 "C": {
															"begin": 0,
															"end": 0,
															"stepsize": 4.0},
													 "G1": {
															"begin": -1500,
															"end": -1500,
															"stepsize": 2.0},
													 "G2": {
															"begin": -1200,
															"end": -1200,
															"stepsize": 2.0},
													 "G3": {
															"begin": -1300,
															"end": -1300,
															"stepsize": 20.0},
													 "G4": {
															"begin": -1700,
															"end": -1700,
															"stepsize": 5},
													 "R": {
															"begin": -4000,
															"end": -4000,
															"stepsize": 5}																
													}
								}
						}
				},
				  
  "Loops": {
			"use qcodes": true,
			"location": "custom_loops.NELoops",
			"class": "NELoops",
			"inner loop":"Loop_Idc",
			"outer loop":"Loop_2D",
			"subscriber delay": 0.1,
			"integration delay": 30e-3
			},
			
  "Print data": false,
  "Data saving":{
			"use datahandler": true,
			"rate": 1.0}
}
