{
  "Sample name": "imec QBC3 - chip3",
  "Comments": "VSD=10 mv", 
  
  "Instruments": { 

	  "Classes":{
				"NEtransport":{
								  "location": "custom_loops.NEInstruments"
								}
				},
				
	  "Inputs": {
		  "S3b":{	  
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["VSDe", "Pcoarse"],
			  "DAC number": [2, 1],
			  "gains": [0.1, 5.0],
			  "unit": "mV",
			  "metaparameters":{
								"rate delay": [0.5] } 
			},

		  "S2d":{
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["LL", "RL", "Pfine", "LB", "RB", "C", "G1", "G2", "G3", "G4", "R"],
			  "DAC number": [12, 13, 14, 15, 16, 4, 5, 6, 7, 8, 9],
			  "gains": [5.0, 5.0, 0.05, 1.0, 1.0, 1.0, 5.0, 5.0, 5.0, 5.0, 5.0],
			  "unit": "mV",
			  "metaparameters":{
								"rate delay": [0.5, 0.5] } 
			} 
				},
					
	  "Outputs":{
		  "Keithley4200":{
					  "class": "NEtransport",
					  "component": "keith1",
					  "name": "Idc1",
					  "gain": 1e-1,
					  "label": "SD current", 
					  "unit": "nA",
					  "metaparameters":{} }, 
		  "1_SR830":{
					"class": "NEtransport",
					"component": "lock_sensor",
					"parameters": ["R", "P"],
					"labels": ["R_sensor", "Ph_sensor"],
					"units": ["V", "deg"],
					"init":{
							"reference_source": "internal",
							"frequency": 333.0,
							"amplitude": 50e-3,
							"time_constant": 30e-3,
							"reserve": "normal",
							"phase": "auto"
							} },
		  "2_SR830":{
					"class": "NEtransport",
					"component": "lock_pulse",
					"parameters": ["R", "P"],
					"labels": ["R_pulse", "Ph_pulse"],
					"units": ["V", "deg"],
					"init":{
							"reference_source": "external",
							"time_constant": 30e-3,
							"reserve": "normal",
							"phase": "auto"
							}	}						
				 }
			},

