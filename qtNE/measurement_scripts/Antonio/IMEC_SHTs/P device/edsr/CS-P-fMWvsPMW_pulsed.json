{
  "Sample name": "QB16_3_4",
  "Comments": "50ns MW pulse, 50ns wait, 1GV/A *1 low Noise",
  
  "Instruments": { 
  
	  "Classes":{
				"NEtransport":{
								  "location": "custom_loops.NEInstruments"
								},
				"NEmagnetic":{
								  "location": "custom_loops.NEInstruments",
								  "parameters needed": ["inputs", "outputs"]
								},
				"NErf":{
								  "location": "custom_loops.NEInstruments",
								  "init":{
											"pulser": "awg",
											"mw": "psg"
										}
						}
				},
				
  	  "Supporting Instruments": {
				},
				
	  "Inputs": {
		  "S3b":{	  
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["VSDh"],
			  "DAC number": [2],
			  "gains": [0.1],
			  "unit": "mV",
			  "metaparameters":{} 
			},

		  "S2d":{
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["LL","RL","P","RB","LB"],
			  "DAC number": [10,11,12,13,14],
			  "gains": [5.0, 5.0, 5.0, 1.0, 1.0],
			  "unit": "mV",
			  "metaparameters":{} 
			},

		  "MercuryiPS":{
			  "class": "NEmagnetic",
			  "component": "magnet",
			  "coordinates": "cartesian",
			  "IDs": ["Bx", "By", "Bz"],
			  "init":{
						"x_ramp_rate":0.1,
						"y_ramp_rate":0.05,
						"z_ramp_rate":0.1}},

		  "AgilentE8257D":{
			  "class": "NErf",
			  "component": "psg",
			  "IDs": ["frequency","power","phase"],
			  "init":{
						"RF": "ON",
						"MOD": "ON"}}
				},

	  "Outputs":{
		  "Keithley4200":{
					  "class": "NEtransport",
					  "component": "keith1",
					  "name": "Idc1",
					  "gain": 1,
					  "label": "SD current", 
					  "unit": "nA",
					  "metaparameters":{"nplc": 5,
										"averaging_type": "moving",
										"averaging_count": 6}
						}
				  }},

  "Experiment":{
											
      "Sweep loop":{
					"ID": ["frequency"],
                    "sweepback": false
                    },
      "Step loop":{
                    "to use": true,	  	
                    "ID": ["power"]
                  },
				  
	  
	  "Class settings":{
				  "NEtransport":{
								  "ranges": {
													 "VSDh": {
															"begin": 2,
															"end": 2,
															"stepsize": 0.1},
													 "LL": {
															"begin": -2770,
															"end": -2770,
															"stepsize": 20},
													 "RL": {
															"begin": -2570,
															"end": -2570,
															"stepsize": 20},
													 "P": {
															"begin": -1280,
															"end": -1280,
															"stepsize": 20},
													 "RB": {
															"begin": -611.9,
															"end": -611.9,
															"npts": 100},
													 "LB": {
															"begin": -678.2,
															"end": -678.2,
															"npts": 100}
													}
								},
				  "NEmagnetic": {
								"ranges": {
													"Bx":{
														"begin": 0.0,
														"end": 0.0,
														"stepsize": 0.01},
													"By":{
														"begin": 0.920,
														"end": 0.920,
														"stepsize": 0.002},
													"Bz":{
														"begin": 0.0,
														"end": 0.0,
														"stepsize": 0.02}
										 }
							  },
				  "NErf": {
								"ranges": {
													"frequency":{
														"begin": 17.65e9,
														"end": 17.85e9,
														"stepsize": 0.001e9},
													"power":{
														"begin": -16,
														"end": 4,
														"stepsize": 0.2
													},
													"phase":{
														"begin": 0,
														"end": 0,
														"stepsize": 1
													}
									 }
						  }
						}
				},
				
  "Averaging":{
  			"integration delay": 0,
			"averaging_time": 0.5,
			"referencing_condition": "NErf.mw.frequency(10.0e9)" 
			  },
			  
  "Loops": {
			"use qcodes": true,
			"location": "custom_loops.NELoops",
			"class": "NELoops",
			"inner loop":"Loop_Idc",
			"outer loop":"Loop_2D"
			},
			
  "Print data": true,
  "Data saving":{
			"use datahandler": true,
			"rate": 1.0}
}
