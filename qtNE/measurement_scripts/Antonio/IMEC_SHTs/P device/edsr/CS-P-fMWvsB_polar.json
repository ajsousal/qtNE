{
  "Sample name": "QB16_3_4",
  "Comments": "CW MW pulse, -500mV on RB",
  
  "Instruments": { 
  
	  "Classes":{
				"NEtransport":{
								  "location": "custom_loops.NEInstruments"
								},
				"NEmagnetic":{
								  "location": "custom_loops.NEInstruments",
								  "parameters needed": ["inputs", "outputs"],
								  "init":{
											"instrument": "magnet"
										 }
								},
				"NErf":{
								  "location": "custom_loops.NEInstruments"}
				},
				
  	  "Supporting Instruments": {
				},
				
	  "Inputs": {
		  "S3b":{	  
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["VSDh"],
			  "DAC number": [2],
			  "gains": [0.1],
			  "unit": "mV",
			  "metaparameters":{} 
			},

		  "S2d":{
			  "class": "NEtransport",
			  "component": "ivvi",
			  "IDs": ["LL","RL","P","RB","LB"],
			  "DAC number": [10,11,12,13,14],
			  "gains": [5.0, 5.0, 5.0, 1.0, 1.0],
			  "unit": "mV",
			  "metaparameters":{} 
			},

		  "MercuryiPS":{
			  "class": "NEmagnetic",
			  "component": "magnet",
			  "coordinates": "spherical",
			  "IDs": ["r", "theta", "phi"],
			  "init":{
						"x_ramp_rate":0.05,
						"y_ramp_rate":0.05,
						"z_ramp_rate":0.05}},

		  "AgilentE8257D":{
			  "class": "NErf",
			  "component": "psg",
			  "IDs": ["frequency","power","phase"],
			  "init":{
						"RF": "ON",
						"MOD": "OFF"}}
				},

	  "Outputs":{
		  "Keithley4200":{
					  "class": "NEtransport",
					  "component": "keith1",
					  "name": "Idc1",
					  "gain": 1,
					  "label": "SD current", 
					  "unit": "nA",
					  "metaparameters":{"nplc": 5,
										"averaging_type": "moving",
										"averaging_count": 6}
						}
				  }},

  "Experiment":{
											
      "Sweep loop":{
					"ID": ["frequency"],
                    "sweepback": false
                    },
      "Step loop":{
                    "to use": true,	  	
                    "ID": ["r"]
                  },
				  
	  
	  "Class settings":{
