															"stepsize": 20},
													 "RB": {
															"begin": -608.2,
															"end": -608.2,
															"npts": 100},
													 "LB": {
															"begin": -676.6,
															"end": -676.6,
															"npts": 100}
													}
								},
				  "NEmagnetic": {
